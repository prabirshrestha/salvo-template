{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "groupName": "sqlx",
      "matchPackageNames": [
        "/sqlx/",
        "/sea-query/",
        "/sea-query-binder/"
      ]
    },
    {
      "groupName": "vergen",
      "matchPackageNames": [
        "/vergen.*/"
      ]
    }
  ]
}
